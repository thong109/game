              .p-banner-top-visual__image-wrapper  
                img.u-image(src=imgPC+"character_visual_05.png" alt="")
            .p-banner-top-visual__background-image.js-effect-eyesfollow
              .p-banner-top-visual__image-wrapper  
                img.u-image(src=imgPC+"character_visual_06.png" alt="")
        .p-banner-top-visual__wrapper.u-display-sp
          img.u-image(src=imgSP+"logo_visual_01.png" alt="Logo")
      ul.p-list-top-system
        li.p-list-top-system__item
          a.p-button-top-system.u-effect-hover(href="#" target="_blank" rel="noopener")
